{
  "version": 2,
  "builds": [
    { "src": "Server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/Server.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/Server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Server.js"
    }
  ],
  "rewrites": [
    { "source": "/api-docs", "destination": "/api/api-docs" },
    { "source": "/swagger.json", "destination": "/api/swagger.json" }
  ]
}
